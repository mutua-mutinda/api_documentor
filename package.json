{
  "name": "api_documentor",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.3",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:debug-brk": "NODE_OPTIONS='--inspect-brk' next dev --turbopack",
    "dev:profile": "NODE_OPTIONS='--prof' next dev --turbopack",
    "dev:heap": "NODE_OPTIONS='--inspect --max-old-space-size=4096' next dev --turbopack",
    "build": "next build",
    "build:debug": "NODE_OPTIONS='--inspect' next build --turbopack",
    "build:analyze": "ANALYZE=true next build --turbopack",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next start",
    "lint": "biome check",
    "format": "biome format --write",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "dev:clean": "npm run clean && npm run dev",
    "turbo:trace": "NODE_OPTIONS='--trace-warnings' next dev --turbopack --experimental-build-trace"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^5.0.2",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@scalar/api-reference-react": "^0.7.53",
    "@scalar/nextjs-api-reference": "^0.8.17",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "tailwindcss": "^4",
    "turbo": "^2.5.8",
    "typescript": "^5"
  }
}
